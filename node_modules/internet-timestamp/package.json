{
    "name": "internet-timestamp",
    "version": "0.0.1",
    "description": "generate rfc3339-compliant timestamps from date objects",
    "main": "index.js",
    "devDependencies": {
        "tap": "~0.4.0",
        "tape": "~0.3.0"
    },
    "scripts": {
        "test": "tap test/*.js"
    },
    "testling" : {
        "files" : "test/*.js",
        "browsers" : [
            "ie/6..latest",
            "chrome/20..latest",
            "firefox/10..latest",
            "safari/latest",
            "opera/11.0..latest",
            "iphone/6", "ipad/6"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/substack/internet-timestamp.git"
    },
    "homepage": "https://github.com/substack/internet-timestamp",
    "keywords": [
        "rfc3339",
        "timestamp",
        "date",
        "rss"
    ],
    "author": {
        "name": "James Halliday",
        "email": "mail@substack.net",
        "url": "http://substack.net"
    },
    "license": "MIT"
}
